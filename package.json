{
  "name": "rr-cli-tw",
  "version": "0.0.4",
  "description": "Rokas Rudgalvis CLI command 'rr-tw' to automate the process of installing Tailwind CSS on SvelteKit projects",
  "author": "Rokas Rudgalvis",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "unlink": "npm unlink rr-cli-tw",
    "link": "npm run unlink && npm link --local"
  },
  "bin": {
    "rr-tw": "dist/bin/install-tailwind-on-svelte/index.js"
  },
  "devDependencies": {
    "@types/chalk-animation": "^1.6.3",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.25",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chalk-animation": "^2.0.3",
    "inquirer": "^9.2.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rudgalvis/rr-cli-tw"
  },
  "keywords": [
    "utilities",
    "svelte",
    "sveltekit",
    "tailwind",
    "tailwindcss",
    "cli",
    "rr-cli-tw",
    "rr-cli"
  ]
}
